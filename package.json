{
  "name": "react-autorun",
  "description": "A macro that compiles into a dependencies array for hooks.",
  "version": "0.0.1",
  "main": "runtime",
  "files": [
    "package.json",
    "README.md",
    "babel/**/*.ts",
    "!babel/**/*.test.ts",
    "babel/index.js",
    "babel/index.js.map",
    "runtime/**/*.ts",
    "!runtime/**/*.test.ts",
    "runtime/index.js",
    "runtime/index.js.map"
  ],
  "scripts": {
    "build": "node -r @esbuild-kit/cjs-loader scripts/build.ts",
    "transform": "node -r @esbuild-kit/cjs-loader scripts/transform.ts",
    "test": "node -r @esbuild-kit/cjs-loader --test **/*.test.ts",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@babel/generator": "^7.21.5",
    "@babel/traverse": "^7.21.5",
    "@babel/types": "^7.21.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/parser": "^7.21.8",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^20.2.1",
    "esbuild": "^0.17.19"
  }
}
